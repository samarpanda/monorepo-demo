{
  "extends": ["../../.eslintrc", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "globals": {
    "document": true,
    "window": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["html", "react"],
  "rules": {
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
  }
}
